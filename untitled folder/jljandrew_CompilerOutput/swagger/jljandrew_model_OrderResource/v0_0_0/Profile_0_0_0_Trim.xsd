<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.opentravel.org/OTM/profile/v0"
    version="0.0.0" xmlns:fin="http://www.opentravel.org/OTM/finance/v0"
    xmlns:ota2="http://www.opentravel.org/OTM/Common/v0"
    xmlns:ota2-0200="http://www.opentravel.org/OTM/Common/v2"
    xmlns:otm="http://www.OpenTravel.org/ns/OTA2/AppInfo_v01_00"
    xmlns:prof="http://www.opentravel.org/OTM/profile/v0" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    
    <xsd:annotation>
        <xsd:appinfo>
            <otm:Library>
                <otm:LibraryName>Profile</otm:LibraryName>
                <otm:LibraryVersion>0.0.0</otm:LibraryVersion>
                <otm:LibraryStatus>DRAFT</otm:LibraryStatus>
                <otm:SourceFile>/Profile_0_0_0.otm</otm:SourceFile>
                <otm:CompilerVersion>4.0-SNAPSHOT</otm:CompilerVersion>
                <otm:CompileDate>2016-05-18T15:17:40.773-04:00</otm:CompileDate>
            </otm:Library>
        </xsd:appinfo>
    </xsd:annotation>
    
    <xsd:import namespace="http://www.opentravel.org/OTM/Common/v2" schemaLocation="Common_2_0_0_Trim.xsd"/>
    <xsd:import namespace="http://www.opentravel.org/OTM/finance/v0" schemaLocation="Finance_0_0_0_Trim.xsd"/>
    <xsd:import namespace="http://www.opentravel.org/OTM/Common/v0" schemaLocation="ota2_imports.xsd"/>
    <xsd:import namespace="http://www.opentravel.org/OTM/Common/v0" schemaLocation="ota2_imports.xsd"/>
    <xsd:import namespace="http://www.opentravel.org/OTM/Common/v0" schemaLocation="ota2_imports.xsd"/>
    <xsd:import
        namespace="http://www.OpenTravel.org/ns/OTA2/AppInfo_v01_00" schemaLocation="built-ins/OTA2_AppInfo_v1.0.0.xsd"/>
    
    <xsd:complexType name="CitizenCountryCode">
        <xsd:annotation>
            <xsd:documentation source="Description">A 2 character country code as defined in ISO3166.</xsd:documentation>
            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}ISO3166</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">CitizenCountryCode</otm:OTA2Entity>
                <otm:Equivalent context="profile">CitizenCountryNameGroup</otm:Equivalent>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ota2:Empty">
                <xsd:attribute name="defaultInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, indicates a default value should be used.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}boolean</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:element name="Document" type="prof:Document"/>
    
    <xsd:complexType name="Document">
        <xsd:annotation>
            <xsd:documentation source="Description">Provides information on a specific document.</xsd:documentation>
            <xsd:documentation source="Description">Provides information on a specific document.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Document</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="9" minOccurs="0"
                name="DocLimitations" type="ota2:String_Short">
                <xsd:annotation>
                    <xsd:documentation source="Description">Used to indicate any limitations on the document (e.g. as a person may only be allowed to spend a max of 30 days in country on a visitor's visa).</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to64</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">DocLimitations</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="100" minOccurs="0"
                name="AdditionalName" type="ota2:String_Short">
                <xsd:annotation>
                    <xsd:documentation source="Description">The name of an additional person listed on this document.</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}AdditionalPersonNames</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">AdditionalPersonNames</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="EffectiveExpireDate" type="ota2:Date_EffectiveExpire">
                <xsd:annotation>
                    <xsd:documentation source="Description">The effective and expiry date of the document.</xsd:documentation>
                    <xsd:documentation source="Implementer">AttributeGroup: EffectiveExpireOptionalDateGroup</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Privacy" type="ota2:Privacy">
                <xsd:annotation>
                    <xsd:documentation source="Description">Allows for control of the sharing of document data between parties.</xsd:documentation>
                    <xsd:documentation source="Implementer">AttributeGroup: PrivacyGroup</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="DocType" type="ota2:DocumentType_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Indicates the type of document (e.g. Passport, Military ID, Drivers License, national ID, Vaccination Certificate). Refer to OpenTravel Code List Document Type (DOC).</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}OTA_CodeType</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">DocType</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="docIssueAuthority" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Indicates the group or association that granted the document.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to64</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">DocIssueAuthority</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="docIssueLocation" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Indicates the location where the document was issued.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to64</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">DocIssueLocation</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="docID" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Unique number assigned by authorities to document.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to32</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">DocID</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="gender" type="ota2:Gender_Enum" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The gender of the document holder.</xsd:documentation>
                <xsd:documentation source="Implementer">AttributeGroup: GenderGroup</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="birthDate" type="xsd:date" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The birth date of the document holder.</xsd:documentation>
                <xsd:documentation source="Implementer">AttributeGroup: BirthDateGroup</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="docIssueStateProv"
            type="ota2:StateProv_Code" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">State or Province where the document was issued.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StateProvCodeType</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">DocIssueStateProv</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="docIssueCountry"
            type="ota2:Country_Code_ISO3166" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Country where the document was issued.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}ISO3166</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">DocIssueCountry</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="birthCountry"
            type="ota2:Country_Code_ISO3166" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Specifies the birth country of the document holder.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}ISO3166</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">BirthCountry</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="birthPlace" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Specifies the birth place of the document holder (e.g., city, state, county, province), when designating a country of birth, use BirthCountry.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to64</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">BirthPlace</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="docHolderNationality"
            type="ota2:Country_Code_ISO3166" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The country code of the nationality of the document holder.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}ISO3166</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">DocHolderNationality</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="contactName" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Provides contact name associated with the document.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to64</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">ContactName</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="holderType" type="prof:DocumentHolder_Enum" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Provides the type of document holder.</xsd:documentation>
                <xsd:documentation source="Implementer">LocalAnonymousType: DocumentType_HolderType</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">HolderType</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="remark" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A remark associated with the document.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to128</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">Remark</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="postalCode" type="ota2:String_Tiny" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Defines the postal code (e.g., ZIP code) on the document.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to16</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">PostalCode</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="RelatedTraveler" type="prof:RelatedTraveler"/>
    
    <xsd:complexType name="RelatedTraveler">
        <xsd:annotation>
            <xsd:documentation source="Description">Other traveler profiles associated with an specific individual.</xsd:documentation>
            <xsd:documentation source="Description">Other traveler profiles associated with an specific individual.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">RelatedTraveler</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="0" ref="prof:PersonSubGrp">
                <xsd:annotation>
                    <xsd:documentation source="Description">Person associated with the traveler.</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}PersonNameType</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">PersonName</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Privacy" type="ota2:Privacy">
                <xsd:annotation>
                    <xsd:documentation source="Description">Allows for control of the sharing of related traveler data between parties.</xsd:documentation>
                    <xsd:documentation source="Implementer">AttributeGroup: PrivacyGroup</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="relation" type="ota2:String_Tiny" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Indicates the type of relationship with the person in the profile, such as Spouse, Child, Family, Business Associate, Interest Group, Medical, Security, Other, etc.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to32</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">Relation</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:simpleType name="DocumentHolder_Enum">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="EnumerationClosed">DocumentHolder_Enum</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Infant">
                <xsd:annotation>
                    <xsd:documentation source="Description">The document holder is an infant.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="HeadOfHousehold">
                <xsd:annotation>
                    <xsd:documentation source="Description">The document holder is a head of household.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:complexType name="LoyaltyProgram">
        <xsd:annotation>
            <xsd:documentation source="Description">Identifies a membership or loyalty program offered by the company by name of the program.</xsd:documentation>
            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength0to32</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">LoyaltyProgram</otm:OTA2Entity>
                <otm:Equivalent context="profile">LoyaltyProgramType</otm:Equivalent>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ota2:Empty">
                <xsd:attribute name="programCode"
                    type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Identification of the loyalty program by code.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to16</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="profile">ProgramCode</otm:Equivalent>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="loyaltyLevel"
                    type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Indicates special privileges in program assigned to individual.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to16</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="profile">LoyaltyLevel</otm:Equivalent>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="id" type="xsd:ID" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A unique reference for a loyalty program within this message.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}RPH_Type</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="profile">RPH</otm:Equivalent>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="codeContext"
                    type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Identifies the source authority for the code.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="primaryInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, indicates this is the primary loyalty program and when absent or false, indicates this is not the primary loyalty program.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}boolean</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="singleVendorIndGroupInd"
                    type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Present and true Indicates the program is part of an alliance, otherwise it is a single vendor.</xsd:documentation>
                        <xsd:documentation source="Implementer">AttributeGroup: SingleVendorIndGroup</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="PhysicalChallenge">
        <xsd:annotation>
            <xsd:documentation source="Description">Describes a person's physical challange.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">PhysicalChallenge</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ota2:String_Long">
                <xsd:attribute name="code" type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">A code to identify a person's phyical challenge.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="physicallyChallengedInd"
                    type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Set to true when the customer is physically challenged but the type of physical challenge is unknown.  The indicator is not necessary if the challenge is identified.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:element name="TravelArranger" type="prof:TravelArranger"/>
    
    <xsd:simpleType name="TravelArranger_Simple">
        <xsd:annotation>
            <xsd:documentation source="Description">A short string  
Minimum length of 0 and a maximum length of 128</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TravelArranger</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="ota2:String_Short"/>
    </xsd:simpleType>
    
    <xsd:simpleType name="TravelArranger_Simple_List">
        <xsd:list itemType="prof:TravelArranger_Simple"/>
    </xsd:simpleType>
    
    <xsd:complexType name="TravelArranger">
        <xsd:annotation>
            <xsd:documentation source="Description">Type of service making travel plans or transactions (e.g., travel agency, etc.).</xsd:documentation>
            <xsd:documentation source="Description">Type of service making travel plans or transactions (e.g., travel agency, etc.).</xsd:documentation>
            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to32</xsd:documentation>
            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to32</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TravelArranger</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="CompanyName" type="ota2:CompanyName">
                <xsd:annotation>
                    <xsd:documentation source="Description">Company name of the travel arranger.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Privacy" type="ota2:Privacy">
                <xsd:annotation>
                    <xsd:documentation source="Description">Allows for control of the sharing of travel arranger data between parties.</xsd:documentation>
                    <xsd:documentation source="Implementer">AttributeGroup: PrivacyGroup</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="type" type="ota2:String_Tiny" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Type of service making travel plans or transactions (e.g., travel agency, etc.).</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to32</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">TravelArrangerType</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="remark" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A remark associated with the travel arranger.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to128</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">Remark</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="id" type="xsd:ID" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A unique reference for a travel arranger within this message.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}RPH_Type</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">RPH</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="defaultInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Indicates that the receiving system should assume the default value if the user specifies no overriding value or action.</xsd:documentation>
                <xsd:documentation source="Implementer">AttributeGroup: DefaultIndGroup</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="Trip_Purpose" type="prof:Trip_Purpose"/>
    
    <xsd:simpleType name="Trip_Purpose_Simple">
        <xsd:annotation>
            <xsd:documentation source="Description">A short string  
Minimum length of 0 and a maximum length of 128</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Trip_Purpose</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="ota2:String_Short"/>
    </xsd:simpleType>
    
    <xsd:simpleType name="Trip_Purpose_Simple_List">
        <xsd:list itemType="prof:Trip_Purpose_Simple"/>
    </xsd:simpleType>
    
    <xsd:complexType name="Trip_Purpose">
        <xsd:annotation>
            <xsd:documentation source="Description">A trip purpose code used by this company.</xsd:documentation>
            <xsd:documentation source="Description">A trip purpose code used by this company.</xsd:documentation>
            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to8</xsd:documentation>
            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to8</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Trip_Purpose</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" ref="prof:CompanySubGrp">
                <xsd:annotation>
                    <xsd:documentation source="Description">Company that defines the list of purpose codes.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="code" type="ota2:String_Tiny" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A code identifiying the purpose of the trip.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="description" type="ota2:String_Text" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A description of the trip purpose.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to128</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">Description</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="Trip_Purposes" type="prof:Trip_Purposes"/>
    
    <xsd:complexType name="Trip_Purposes">
        <xsd:annotation>
            <xsd:documentation source="Description">A container for repeating group of trip purpose codes and descriptions.</xsd:documentation>
            <xsd:documentation source="Description">A container for repeating group of trip purpose codes and descriptions.</xsd:documentation>
