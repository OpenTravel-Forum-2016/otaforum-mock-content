            </xsd:element>
            <xsd:element minOccurs="1" ref="ota2:Person_Name">
                <xsd:annotation>
                    <xsd:documentation source="Description">This provides name information for a person.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="ProfileSubGrp" type="prof:Profile_ID">
        <xsd:annotation>
            <xsd:documentation source="Description">Root element for profile content.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Profile</otm:OTA2Entity>
