    <xsd:element name="OrganizationMemberName" type="prof:OrganizationMemberName"/>
    
    <xsd:complexType name="OrganizationMemberName">
        <xsd:annotation>
            <xsd:documentation source="Implementer">Extension: PersonNameType</xsd:documentation>
            <xsd:documentation source="Implementer">Extension: PersonNameType</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">OrganizationMemberName</otm:OTA2Entity>
