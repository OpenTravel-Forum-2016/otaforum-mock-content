                            <xsd:documentation source="Implementer">AttributeGroup: DateTimeStampGroup</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
