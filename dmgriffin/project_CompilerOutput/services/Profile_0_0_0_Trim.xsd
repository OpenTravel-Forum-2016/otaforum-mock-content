    <xsd:complexType name="Company_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Company</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Identifier" type="ota2:Identifier">
                <xsd:annotation>
                    <xsd:documentation source="Description">Identifier provides the ability to create a globally unique identifier. For the identifier to be globally unique it must have a system provided identifier and the system must be identified using a global naming authority. System identification uses the domain naming system (DNS) to assure they are globally unique and should be an URL. The system provided ID will typically be a primary or surrogate key in a database.

The URL, system and company attributes can be omitted only when the system context can be implied by a parent or ancestor element.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:ID" use="optional"/>
    </xsd:complexType>
    
    <xsd:complexType name="Company">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Company</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prof:Company_ID">
                <xsd:sequence>
                    <xsd:element maxOccurs="20" minOccurs="1"
                        name="Name" type="ota2:CompanyName">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Detailed name information for the company.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}CompanyNameType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="Common">CompanyName</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="100" minOccurs="0" ref="ota2:AddressSubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">A jurisdiction in which a company is authorized to do business. Uses the Address entity to indicate the location of business using any of the components of the address (e.g., city, state, zip code, country).</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}AddressType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="Common">BusinessLocale</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="100" minOccurs="0" ref="ota2:TelephoneSubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Information about a telephone number, including the actual number and its usage.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="100" minOccurs="0"
                        name="Email" type="ota2:Email">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Electronic email addresses, in IETF specified format.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="100" minOccurs="0"
                        name="URL" type="ota2:URL">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Information on a URL for the company.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}URL_Type</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="Common">URL</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Summary"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="Company_Detail">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Company</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prof:Company">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="CurrencyCode" type="ota2:Currency_Code">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The currency used by this company.</xsd:documentation>
                            <xsd:documentation source="Implementer">AttributeGroup: CurrencyCodeGroup</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="1" minOccurs="0" ref="prof:Trip_Purposes">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Trip purpose codes used by this company.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}TripPurpose</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">TripPurpose</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="10" minOccurs="0"
                        name="LoyaltyProgram" type="prof:LoyaltyProgram">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Loyalty program offered by the company.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}LoyaltyProgramType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">LoyaltyProgram</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="1" minOccurs="0" ref="prof:TravelArranger">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Information on the organization responsible for arranging travel for the company.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}TravelArrangerType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">TravelArranger</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="1" minOccurs="0"
                        name="RequiredPayment" type="ota2:RequiredPayments_Enum">
                        <xsd:annotation>
                            <xsd:documentation source="Description">A deposit policy that is associated to this company.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}RequiredPaymentsType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">DepositPayment</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="1" minOccurs="0" ref="fin:FormOfPaymentSubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Method of providing funds for travel by the company.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}PaymentForm</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">PaymentForm</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="100" minOccurs="0"
                        name="RelatedCompany" type="ota2:CompanyName">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The name of a company affiliated with the employer.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}CompanyNameType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">RelatedEmployer</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="100" minOccurs="0" ref="ota2-0200:ContactPerson">
                        <xsd:annotation>
                            <xsd:documentation source="Description">A contact person for the company.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}ContactPersonType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">ContactPerson</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Detail"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="Company_Employer">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Company</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prof:Company">
                <xsd:sequence>
                    <xsd:element maxOccurs="1" minOccurs="0"
                        name="EmployeeInfo" type="ota2:Employee">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Information about the employee.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}EmployeeInfoType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">EmployeeInfo</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="1" minOccurs="0"
                        name="InternalReferenceNumber" type="ota2:String_Short">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Accounting code(s) assigned to travel for employer.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}FreeTextType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">InternalRefNmbr</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="1" minOccurs="0" ref="prof:TravelArranger">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The organization responsible for arranging travel for the employer.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}TravelArrangerType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">TravelArranger</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="10" minOccurs="0"
                        name="LoyaltyProgram" type="prof:LoyaltyProgram">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Employer's loyalty program information.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}LoyaltyProgramType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">LoyaltyProgram</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0"
                        name="EffectiveExpireDate" type="ota2:Date_EffectiveExpire">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Used to send the effective date and/or expiration date.</xsd:documentation>
                            <xsd:documentation source="Implementer">AttributeGroup: EffectiveExpireOptionalDateGroup</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Custom"/>
                </xsd:sequence>
                <xsd:attribute name="officeType"
                    type="ota2:OfficeLocation_Enum" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Designates the office category within an organization.</xsd:documentation>
                        <xsd:documentation source="Implementer">AttributeGroup: OfficeTypeGroup</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="division" type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The division name or ID with which the contact is associated.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="department" type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The department name or ID with which the contact is associated.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="defaultInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Implementer">AttributeGroup: DefaultIndGroup</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="PersonSubGrp" type="prof:Person_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Person</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PersonID" substitutionGroup="prof:PersonSubGrp" type="prof:Person_ID"/>
    <xsd:element name="PersonIdentifier"
        substitutionGroup="prof:PersonSubGrp" type="prof:Person_ID"/>
    <xsd:element name="Person" substitutionGroup="prof:PersonID" type="prof:Person"/>
    <xsd:element name="PersonSummary"
        substitutionGroup="prof:PersonSubGrp" type="prof:Person"/>
    <xsd:element name="PersonDetail" substitutionGroup="prof:Person" type="prof:Person_Detail"/>
    <xsd:element name="PersonCustomer" substitutionGroup="prof:Person" type="prof:Person_Customer"/>
    
    <xsd:complexType name="Person_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Person</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="Identifier" type="ota2:Identifier">
                <xsd:annotation>
                    <xsd:documentation source="Description">An identifier used to uniquely reference an object in a system (e.g. an airline reservation reference, customer profile reference, booking confirmation number, or a reference to a previous availability quote).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="5" minOccurs="0" name="OtherID" type="ota2:Identifier">
                <xsd:annotation>
                    <xsd:documentation source="Description">These identifiers are assigned by systems other than the system that generated this record.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:ID" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Provides a unique reference to this contact person.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}RPH_Type</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="Common">RPH</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="Person">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Person</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prof:Person_ID">
                <xsd:sequence>
                    <xsd:element maxOccurs="1" minOccurs="1" ref="ota2:Person_Name">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Name information for a person.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}PersonNameType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="Common">PersonName</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="5" minOccurs="0" ref="ota2:AddressSubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Information about an address that identifies a location for a specific purposes.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}AddressInfoType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="Common">Address</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:TelephoneSubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Information about a telephone number, including the actual number and its usage.</xsd:documentation>
                            <xsd:documentation source="Implementer">Example value: p = 555-555-5555</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="5" minOccurs="0"
                        name="Email" type="ota2:Email">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Electronic email addresses, in IETF specified format.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}EmailType</xsd:documentation>
                            <xsd:documentation source="Implementer">Example value: p = john.smith@email.com</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="Common">Email</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Privacy" type="ota2:Privacy">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Allows for control of the sharing of data between parties.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Comments" type="ota2:Text_Free">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Textual information about the person.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Summary"/>
                </xsd:sequence>
                <xsd:attribute name="gender" type="ota2:Gender_Enum" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The gender of the person.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="maritalStatus_Enum"
                    type="ota2:MaritalStatus_Enum" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Specifies the marital status of the person.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="birthDate" type="xsd:date" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The birth date of the person.</xsd:documentation>
                        <xsd:documentation source="Implementer">AttributeGroup: BirthDateGroup</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="Person_Detail">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Person</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prof:Person">
                <xsd:sequence>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" ref="prof:CompanySubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Company or organization that employs the customer.</xsd:documentation>
                            <xsd:documentation source="Implementer">Example value: p = ABC Corporation</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="unbounded" minOccurs="0"
                        name="SocialMediaInfo" type="ota2:SocialMedia">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies social media information used for communication with the person.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}SocialMediaType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">SocialMediaInfo</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="5" minOccurs="0" name="URL" type="ota2:URL">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Web site address, in IETF specified format.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}URL_Type</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">URL</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Detail"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="Person_Customer">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Person</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prof:Person">
                <xsd:sequence>
                    <xsd:element maxOccurs="3" minOccurs="0"
                        name="CitizenCountryCode" type="prof:CitizenCountryCode">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Name of the (self-professed) country that is claimed for citizenship.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}CitizenCountryName</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">CitizenCountryName</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="3" minOccurs="0"
                        name="PetInfo" type="ota2:String_Short">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Describes the customer's pet.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to64</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">PetInfo</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="100" minOccurs="0" ref="fin:FormOfPaymentSubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Methods of providing funds and guarantees for travel by the customer.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}PaymentForm</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">PaymentForm</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="99" minOccurs="0" ref="prof:RelatedTraveler">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Identifies a traveler associated with the customer.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}RelatedTravelerType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">RelatedTraveler</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="5" minOccurs="0" ref="ota2-0200:ContactPerson">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Information on a contact person for the customer.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}ContactPersonType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">ContactPerson</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="99" minOccurs="0" ref="prof:Document">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Detailed document information for the customer (e.g.,  driver license, passport, visa).</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}DocumentType</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">Document</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="25" minOccurs="0"
                        name="CustLoyalty" type="prof:LoyaltyProgram">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Loyalty program information for the customer.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}CustLoyalty</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">CustLoyalty</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="5" minOccurs="0"
                        name="AdditionalLanguages" type="ota2:Languages">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Additional languages spoken by the traveler.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}AdditionalLanguage</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">AdditionalLanguage</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="PhysicalChallenge" type="prof:PhysicalChallenge">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Describes a person physical challange.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Custom"/>
                </xsd:sequence>
                <xsd:attribute name="customerValue"
                    type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The supplier's ranking of the customer (e.g., VIP, numerical ranking).</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to8</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="profile">CustomerValue</otm:Equivalent>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="children" type="xsd:integer" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The number of children of the customer.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}Numeric1to99</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="profile">ChildQuantity</otm:Equivalent>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="deceasedInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true the customer is deceased.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="vIP_Ind" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">If true, indicates a very important person.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="previouslyMarriedInd"
                    type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, indicates the customer was previously married. When false, indicates the customer was not previously married.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="CreateRQ"
        substitutionGroup="ota2msg:OTA2_Request_Payload" type="prof:Create_RQ"/>
    
    <xsd:complexType name="Create_RQ">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Operation">ProfileService_Create</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ota2msg:OTA2_Request_Payload">
                <xsd:sequence>
                    <xsd:element minOccurs="0" ref="prof:ProfileSubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The profile to be created.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="1" minOccurs="0" ref="ota2:POS">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Point of sale object.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.opentravel.org/OTA/2003/05}POS_Type</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">POS</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="1"
                        name="PayloadStdAttributes" type="ota2:PayloadStdAttributes">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The standard attributes that appear on the root element for all OpenTravel messages.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="CreateRS"
        substitutionGroup="ota2msg:OTA2_Response_Payload" type="prof:Create_RS"/>
    
    <xsd:complexType name="Create_RS">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Operation">ProfileService_Create</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ota2msg:OTA2_Response_Payload">
                <xsd:sequence>
                    <xsd:element maxOccurs="1" minOccurs="1" ref="prof:ProfileSubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The profile created by the request,  may only include the identifier for the profile and not the full profile information.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.opentravel.org/OTA/2003/05}UniqueID_Type</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">UniqueID</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0"
                        name="PayloadStdAttributes" type="ota2:PayloadStdAttributes">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The OTA_PayloadStdAttributes defines the standard attributes that appear on the root element for all OpenTravel Messages.</xsd:documentation>
                            <xsd:documentation source="Implementer">AttributeGroup: {http://www.opentravel.org/OTA/2003/05}OTA_PayloadStdAttributes</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:Result">
                        <xsd:annotation>
                            <xsd:documentation source="Description">When present indicates that errors or warnings occurred during processing.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
                </xsd:sequence>
