<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.opentravel.org/OTM/profile/v0"
    version="0.0.0" xmlns:fin="http://www.opentravel.org/OTM/finance/v0"
    xmlns:ota2="http://www.opentravel.org/OTM/Common/v0"
    xmlns:ota2-0200="http://www.opentravel.org/OTM/Common/v2"
    xmlns:ota2msg="http://opentravel.org/common/message/v02"
    xmlns:otm="http://www.OpenTravel.org/ns/OTA2/AppInfo_v01_00"
    xmlns:prof="http://www.opentravel.org/OTM/profile/v0" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    
    <xsd:annotation>
        <xsd:appinfo>
            <otm:Library>
                <otm:ProjectName>project.otp</otm:ProjectName>
                <otm:LibraryName>Profile</otm:LibraryName>
                <otm:LibraryVersion>0.0.0</otm:LibraryVersion>
                <otm:LibraryStatus>DRAFT</otm:LibraryStatus>
                <otm:SourceFile>/Profile_0_0_0.otm</otm:SourceFile>
                <otm:CompilerVersion>4.0-SNAPSHOT</otm:CompilerVersion>
                <otm:CompileDate>2016-05-18T16:11:12.088-04:00</otm:CompileDate>
            </otm:Library>
        </xsd:appinfo>
    </xsd:annotation>
    
    <xsd:import namespace="http://opentravel.org/common/message/v02" schemaLocation="built-ins/OTA2_Message_v2.0.0.xsd"/>
    <xsd:import namespace="http://www.opentravel.org/OTM/Common/v2" schemaLocation="Common_2_0_0_Trim.xsd"/>
    <xsd:import namespace="http://www.opentravel.org/OTM/Common/v0" schemaLocation="ota2_imports.xsd"/>
    <xsd:import namespace="http://www.opentravel.org/OTM/Common/v0" schemaLocation="ota2_imports.xsd"/>
    <xsd:import namespace="http://www.opentravel.org/OTM/Common/v0" schemaLocation="ota2_imports.xsd"/>
    <xsd:import namespace="http://www.opentravel.org/OTM/finance/v0" schemaLocation="Finance_0_0_0_Trim.xsd"/>
    <xsd:import
        namespace="http://www.OpenTravel.org/ns/OTA2/AppInfo_v01_00" schemaLocation="built-ins/OTA2_AppInfo_v1.0.0.xsd"/>
    
    <xsd:complexType name="Access">
        <xsd:annotation>
            <xsd:documentation source="Description">Free text comment added by the person accessing the profile record.</xsd:documentation>
            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}FreeTextType</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">Access</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ota2:Empty">
                <xsd:attribute name="action" type="ota2:AccessType_CRUD" use="required">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Type of action taken on the profile.</xsd:documentation>
                        <xsd:documentation source="Implementer">LocalAnonymousType: AccessesType_ActionType</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="profile">ActionType</otm:Equivalent>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="date" type="xsd:dateTime" use="required">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Month, day, year and optionally in hour, minute, second of day in ISO 8601 format of the action taken on the profile.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}dateTime</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="profile">ActionDateTime</otm:Equivalent>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="name" type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Name of individual who originated or updated record.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}PersonNameType</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="profile">AccessPerson</otm:Equivalent>
                            <otm:Example context="profile">Dave</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="system" type="ota2:GUID" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The identifier of the system that performed the action on the profile record.</xsd:documentation>
                        <xsd:documentation source="Implementer">AttributeGroup: ID_Group</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:element name="AccessesSubGrp" type="prof:Accesses_ID">
        <xsd:annotation>
            <xsd:documentation source="Description">Element to capture creation and last update data.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Accesses</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="AccessesID"
        substitutionGroup="prof:AccessesSubGrp" type="prof:Accesses_ID"/>
    <xsd:element name="AccessesIdentifier"
        substitutionGroup="prof:AccessesSubGrp" type="prof:Accesses_ID"/>
    <xsd:element name="Accesses" substitutionGroup="prof:AccessesID" type="prof:Accesses"/>
    <xsd:element name="AccessesSummary"
        substitutionGroup="prof:AccessesSubGrp" type="prof:Accesses"/>
    <xsd:element name="AccessesDetail" substitutionGroup="prof:Accesses" type="prof:Accesses_Detail"/>
    
    <xsd:complexType name="Accesses_ID">
        <xsd:annotation>
            <xsd:documentation source="Description">Element to capture creation and last update data.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Accesses</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Identifier" type="ota2:Identifier">
                <xsd:annotation>
                    <xsd:documentation source="Description">Identifier provides the ability to create a globally unique identifier. For the identifier to be globally unique it must have a system provided identifier and the system must be identified using a global naming authority. System identification uses the domain naming system (DNS) to assure they are globally unique and should be an URL. The system provided ID will typically be a primary or surrogate key in a database.

The URL, system and company attributes can be omitted only when the system context can be implied by a parent or ancestor element.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="Accesses">
        <xsd:annotation>
            <xsd:documentation source="Description">Element to capture creation and last update data.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Accesses</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prof:Accesses_ID">
                <xsd:sequence>
                    <xsd:element maxOccurs="unbounded" minOccurs="1"
                        name="Access" type="prof:Access">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Describes the action taken on the record.</xsd:documentation>
                            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}Access</xsd:documentation>
                            <xsd:appinfo>
                                <otm:Equivalent context="profile">Access</otm:Equivalent>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Privacy" type="ota2:Privacy">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Allows for control of the sharing of access data between parties.</xsd:documentation>
                            <xsd:documentation source="Implementer">AttributeGroup: PrivacyGroup</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Summary"/>
                </xsd:sequence>
                <xsd:attribute name="created" type="xsd:dateTime" use="required">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Date and time the access group was created: Month, day, year and optionally hour, minute, second of day, in ISO 8601 format.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}dateTime</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="profile">CreateDateTime</otm:Equivalent>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="Accesses_Detail">
        <xsd:annotation>
            <xsd:documentation source="Description">Element to capture creation and last update data.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Accesses</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prof:Accesses">
                <xsd:sequence>
                    <xsd:element minOccurs="1" ref="prof:CompanySubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Detailed information about a company.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Detail"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="Affiliations" type="prof:Affiliations"/>
    
    <xsd:complexType name="Affiliations">
        <xsd:annotation>
            <xsd:documentation source="Description">Defines an entity that provides travel services or benefits to a customer or company.</xsd:documentation>
            <xsd:documentation source="Description">Defines an entity that provides travel services or benefits to a customer or company.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Affiliations</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="prof:Organization">
                <xsd:annotation>
                    <xsd:documentation source="Description">Detailed information for a membership organization that has travel benefits, programs or discounts.</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}OrganizationType</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">Organization</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="prof:CompanySubGrp">
                <xsd:annotation>
                    <xsd:documentation source="Description">Employer information associated to this affiliation.</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}EmployerType</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">Employer</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="prof:TravelArranger">
                <xsd:annotation>
                    <xsd:documentation source="Description">Information on the organization responsible for arranging travel.</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}TravelArrangerType</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">TravelArranger</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ota2:ClubMembership">
                <xsd:annotation>
                    <xsd:documentation source="Description">A club that offers special privileges and related services for this affiliation.</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}TravelClubType</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">TravelClub</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="prof:Insurance">
                <xsd:annotation>
                    <xsd:documentation source="Description">Travel insurance associated to this affiliation.</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}InsuranceType</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">Insurance</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="Privacy" type="ota2:Privacy">
                <xsd:annotation>
                    <xsd:documentation source="Description">Allows for control of the sharing of data between parties.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Agreements" type="prof:Agreements"/>
    
    <xsd:complexType name="Agreements">
        <xsd:annotation>
            <xsd:documentation source="Description">Section of a business profile that contains information about trading partner agreements.</xsd:documentation>
            <xsd:documentation source="Description">Section of a business profile that contains information about trading partner agreements.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Agreements</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0"
                name="Certification" type="prof:Certification">
                <xsd:annotation>
                    <xsd:documentation source="Description">Information about certifications or accreditations.</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}CertificationType</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">Certification</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="prof:Alliance">
                <xsd:annotation>
                    <xsd:documentation source="Description">Information about alliance partnerships and consortiums.</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}AllianceConsortiumType</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">AllianceConsortium</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0"
                name="Commission" type="prof:Commission">
                <xsd:annotation>
                    <xsd:documentation source="Description">Information about an agreement for a commission arrangement.</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}CommissionInfoType</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">CommissionInfo</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" ref="ota2:Text_Paragraph">
                <xsd:annotation>
                    <xsd:documentation source="Description">Used to reflect contract details linked to a profile.</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}ParagraphType</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">ContractInformation</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="Privacy" type="ota2:Privacy">
                <xsd:annotation>
                    <xsd:documentation source="Description">Allows for control of the sharing of data between parties.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Alliance" type="prof:Alliance"/>
    
    <xsd:complexType name="Alliance">
        <xsd:annotation>
            <xsd:documentation source="Description">Provides information about alliance partnerships and consortiums of members grouped together to obtain trading partner agreements for travel service and privileges.</xsd:documentation>
            <xsd:documentation source="Description">Provides information about alliance partnerships and consortiums of members grouped together to obtain trading partner agreements for travel service and privileges.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Alliance</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0"
                name="Member" type="prof:Alliance_Member">
                <xsd:annotation>
                    <xsd:documentation source="Description">Identification of a company that participates in an alliance or consortium to which the primary business entity identified in this profile belongs.</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}AllianceMember</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">AllianceMember</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="EffectiveExpireDate" type="ota2:Date_EffectiveExpire">
                <xsd:annotation>
                    <xsd:documentation source="Description">Provides the effective and expiry dates for the alliance partnership.</xsd:documentation>
                    <xsd:documentation source="Implementer">AttributeGroup: EffectiveExpireOptionalDateGroup</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="Identifier" type="ota2:Identifier">
                <xsd:annotation>
                    <xsd:documentation source="Description">Identifier provides the ability to create a globally unique identifier. For the identifier to be globally unique it must have a system provided identifier and the system must be identified using a global naming authority. System identification uses the domain naming system (DNS) to assure they are globally unique and should be an URL. The system provided ID will typically be a primary or surrogate key in a database.

The URL, system and company attributes can be omitted only when the system context can be implied by a parent or ancestor element.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:ID" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Identification of the alliance or partnership.</xsd:documentation>
                <xsd:documentation source="Implementer">AttributeGroup: ID_OptionalGroup</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="profile">ac3455432112345</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="Alliance_Member">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">Alliance_Member</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ota2:Empty">
                <xsd:attribute name="memberCode"
                    type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Identifies the alliance or consortium member by code.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="profile">co1</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="companyName" type="ota2:String" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Identifies a company by name.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="profile">MemberCompany</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="division" type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The division name or ID with which the contact is associated.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to32</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="Common">Division</otm:Equivalent>
                            <otm:Example context="Common">Travel Division</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="department"
                    type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The department name or ID with which the contact is associated.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to32</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="Common">Department</otm:Equivalent>
                            <otm:Example context="Common">Adventure department</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="shortName" type="ota2:String_Short" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Used to provide the company common name.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to32</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="Common">CompanyShortName</otm:Equivalent>
                            <otm:Example context="Common">Adventure Inc</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="code" type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Identifies a company by the company code.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to16</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="Common">Code</otm:Equivalent>
                            <otm:Example context="Common">AI</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="codeContext"
                    type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Identifies the source authority for the code.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="Certification">
        <xsd:annotation>
            <xsd:documentation source="Description">Information about certifications or accreditation held by the company.</xsd:documentation>
            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}FreeTextType</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">Certification</otm:OTA2Entity>
                <otm:Equivalent context="profile">CertificationType</otm:Equivalent>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ota2:Empty">
                <xsd:attribute name="certId" type="ota2:String_Long" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The Certification Identification number assigned to a business entity by the certifying body. This number would presumably be the ID in the Unique ID.</xsd:documentation>
                        <xsd:documentation source="Implementer">AttributeGroup: ID_OptionalGroup</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="profile">cert546473834</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="freeText" type="ota2:String_Text" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Textual information to provide descriptions and/or additional information.</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="profile">Certified fun.</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="language" type="xsd:language" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Language of the text.</xsd:documentation>
                        <xsd:documentation source="Implementer">AttributeGroup: LanguageGroup</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="Common">en</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="effective" type="xsd:date" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Indicates the starting date.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}date</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="Common">EffectiveDate</otm:Equivalent>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="expire" type="xsd:date" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Indicates the ending date.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}date</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="Common">ExpireDate</otm:Equivalent>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="singleVendorIndGroupInd"
                    type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">TODO - what is this? Indicates the alliance status of the company.</xsd:documentation>
                        <xsd:documentation source="Implementer">AttributeGroup: SingleVendorIndGroup</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="expireDateExclusiveInd"
                    type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, indicates that the ExpireDate is the first day after the applicable period (e.g. when expire date is Oct 15  the last date of the period is Oct 14).</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}boolean</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="CitizenCountryCode">
        <xsd:annotation>
            <xsd:documentation source="Description">A 2 character country code as defined in ISO3166.</xsd:documentation>
            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}ISO3166</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">CitizenCountryCode</otm:OTA2Entity>
                <otm:Equivalent context="profile">CitizenCountryNameGroup</otm:Equivalent>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ota2:Empty">
                <xsd:attribute name="defaultInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, indicates a default value should be used.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}boolean</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="Commission">
        <xsd:annotation>
            <xsd:documentation source="Description">Contains information about agreements for commission arrangements with the business entity.</xsd:documentation>
            <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}FreeTextType</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">Commission</otm:OTA2Entity>
                <otm:Equivalent context="profile">CommissionInfoType</otm:Equivalent>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ota2:Empty">
                <xsd:attribute name="planCode" type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Identifies a commission plan agreement between trading partners by plan code.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to32</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="profile">CommissionPlanCode</otm:Equivalent>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="currency_Amount" type="ota2:Money" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Provides a monetary amount and the currency code to reflect the currency in which this amount is expressed.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="codeContext"
                    type="ota2:String_Tiny" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Identifies the source authority for the code.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="currencyCode"
                    type="ota2:Currency_Code_ISO4217" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">An ISO 4217 (3) alpha character code that specifies a monetary unit.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}AlphaLength3</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="Common">CurrencyCode</otm:Equivalent>
                            <otm:Example context="Common">USD</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="decimalPlaces"
                    type="xsd:positiveInteger" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Implementer: The ISO 4217 standard "minor unit" is typically used when the amount provided includes the minor unit of currency without a decimal point (e.g., USD 8500 needs DecimalPlaces="2" to represent $85).</xsd:documentation>
                        <xsd:documentation source="Implementer">Changed type from {http://www.w3.org/2001/XMLSchema}nonNegativeInteger.</xsd:documentation>
                        <xsd:documentation source="Implementer">Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Equivalent context="Common">DecimalPlaces</otm:Equivalent>
                            <otm:Example context="Common">2</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="shareMarketInd"
                    type="ota2:YesNoInherit_Enum" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Permission for sharing data for marketing purposes.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="shareSyncInd"
                    type="ota2:YesNoInherit_Enum" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">Permission for sharing data for synchronization of information held by other travel service providers.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="optInStatus"
                    type="ota2:OptInStatus_Enum" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">If present and set to opt out, a customer has explicitly opted out of marketing communication. When set to opt out it overrides the ShareMarketInd.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="optInDate" type="xsd:dateTime" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The date and time of opt in.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="optOutDate" type="xsd:dateTime" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The date and time of opt out.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:element name="Document" type="prof:Document"/>
    
    <xsd:complexType name="Document">
        <xsd:annotation>
            <xsd:documentation source="Description">Provides information on a specific document.</xsd:documentation>
            <xsd:documentation source="Description">Provides information on a specific document.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Document</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="9" minOccurs="0"
                name="DocLimitations" type="ota2:String_Short">
                <xsd:annotation>
                    <xsd:documentation source="Description">Used to indicate any limitations on the document (e.g. as a person may only be allowed to spend a max of 30 days in country on a visitor's visa).</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to64</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">DocLimitations</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="100" minOccurs="0"
                name="AdditionalName" type="ota2:String_Short">
                <xsd:annotation>
                    <xsd:documentation source="Description">The name of an additional person listed on this document.</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}AdditionalPersonNames</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">AdditionalPersonNames</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="EffectiveExpireDate" type="ota2:Date_EffectiveExpire">
                <xsd:annotation>
                    <xsd:documentation source="Description">The effective and expiry date of the document.</xsd:documentation>
                    <xsd:documentation source="Implementer">AttributeGroup: EffectiveExpireOptionalDateGroup</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Privacy" type="ota2:Privacy">
                <xsd:annotation>
                    <xsd:documentation source="Description">Allows for control of the sharing of document data between parties.</xsd:documentation>
                    <xsd:documentation source="Implementer">AttributeGroup: PrivacyGroup</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="DocType" type="ota2:DocumentType_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Indicates the type of document (e.g. Passport, Military ID, Drivers License, national ID, Vaccination Certificate). Refer to OpenTravel Code List Document Type (DOC).</xsd:documentation>
                    <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}OTA_CodeType</xsd:documentation>
                    <xsd:appinfo>
                        <otm:Equivalent context="profile">DocType</otm:Equivalent>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="docIssueAuthority" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Indicates the group or association that granted the document.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to64</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">DocIssueAuthority</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="docIssueLocation" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Indicates the location where the document was issued.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to64</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">DocIssueLocation</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="docID" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Unique number assigned by authorities to document.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to32</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">DocID</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="gender" type="ota2:Gender_Enum" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The gender of the document holder.</xsd:documentation>
                <xsd:documentation source="Implementer">AttributeGroup: GenderGroup</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="birthDate" type="xsd:date" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The birth date of the document holder.</xsd:documentation>
                <xsd:documentation source="Implementer">AttributeGroup: BirthDateGroup</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="docIssueStateProv"
            type="ota2:StateProv_Code" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">State or Province where the document was issued.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StateProvCodeType</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">DocIssueStateProv</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="docIssueCountry"
            type="ota2:Country_Code_ISO3166" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Country where the document was issued.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}ISO3166</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">DocIssueCountry</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="birthCountry"
            type="ota2:Country_Code_ISO3166" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Specifies the birth country of the document holder.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}ISO3166</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">BirthCountry</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="birthPlace" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Specifies the birth place of the document holder (e.g., city, state, county, province), when designating a country of birth, use BirthCountry.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to64</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">BirthPlace</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="docHolderNationality"
            type="ota2:Country_Code_ISO3166" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The country code of the nationality of the document holder.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}ISO3166</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">DocHolderNationality</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="contactName" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Provides contact name associated with the document.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to64</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">ContactName</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="holderType" type="prof:DocumentHolder_Enum" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Provides the type of document holder.</xsd:documentation>
                <xsd:documentation source="Implementer">LocalAnonymousType: DocumentType_HolderType</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">HolderType</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="remark" type="ota2:String_Short" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A remark associated with the document.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to128</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">Remark</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="postalCode" type="ota2:String_Tiny" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Defines the postal code (e.g., ZIP code) on the document.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to16</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">PostalCode</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="Insurance" type="prof:Insurance"/>
    
    <xsd:complexType name="Insurance">
        <xsd:annotation>
            <xsd:documentation source="Description">Travel insurance carried by the customer or company.</xsd:documentation>
            <xsd:documentation source="Description">Travel insurance carried by the customer or company.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Insurance</otm:OTA2Entity>
