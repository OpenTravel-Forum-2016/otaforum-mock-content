    <xsd:element name="Profile" substitutionGroup="prof:ProfileID" type="prof:Profile"/>
    <xsd:element name="ProfileSummary"
        substitutionGroup="prof:ProfileSubGrp" type="prof:Profile"/>
    
    <xsd:complexType name="Profile_ID">
        <xsd:annotation>
            <xsd:documentation source="Description">Root element for profile content.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Profile</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Identifier" type="ota2:Identifier">
                <xsd:annotation>
                    <xsd:documentation source="Description">Globally unique identifier for this profile.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:ID" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">This is a reference placeholder, used as an index for this guest in this reservation.</xsd:documentation>
                <xsd:documentation source="Implementer">Assigned XSD Type: {http://chameleon.anonymous/ns}RPH_Type</xsd:documentation>
                <xsd:appinfo>
                    <otm:Equivalent context="profile">RPH</otm:Equivalent>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="Profile">
        <xsd:annotation>
            <xsd:documentation source="Description">Root element for profile content.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Profile</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="prof:Profile_ID">
